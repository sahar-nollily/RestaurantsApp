<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_nav"
    app:startDestination="@id/mapsFragment">

    <fragment
        android:id="@+id/restaurantsApp"
        android:name="com.bignerdranch.android.restaurantsapp.ui.RestaurantsListFragment"
        android:label="fragment_restaurants_app"
        tools:layout="@layout/fragment_restaurants_list" >
        <argument
            android:name="latitude"
            app:argType="string" />
        <argument
            android:name="longitude"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/mapsFragment"
        android:name="com.bignerdranch.android.restaurantsapp.MapsFragment"
        android:label="fragment_maps"
        tools:layout="@layout/fragment_maps" >
        <action
            android:id="@+id/action_mapsFragment_to_restaurantsApp"
            app:destination="@id/restaurantsApp" />
    </fragment>
</navigation>